{
  "name": "squareReader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew bundleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "clean": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-camera-kit": "github:coreyphillips/react-native-camera-kit#v13-patch",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-globalize": "^4.5.1",
    "react-native-permissions": "^4.1.5",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-square-reader-sdk": "^1.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/eslint-config": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.81",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
